
public class TestNumber {

	public static void main(String[] args) {
		int k=1;
		for(int i=1;i<=5;i++)
		{
			if(i%2!=0)
			{
				k=k+i;
				for(int j=1;j<=i;j++)
				{
					System.out.print(k++);
				}
			}
			else
			{
				k=k+i;
				for(int j=1;j<=i;j++)
				{
					System.out.print(k--);
				}
			}
			System.out.println();
		}
	}

	/*public static void main(String[] args) {
