
public class AlternateRowPattern {

	public static void main(String[] args) {
		int k=0;
		
		for(int i=1;i<=4;i++)
		{
			
			
			for(int j=1;j<=i;j++)
			{
				if(i%2!=0){
				System.out.print(k++);
				
				}
				else{
					k=i+k-j; //3,
					System.out.print(k);
				}
			}
			System.out.println();
		}
		
			
	}
}
