package datastructure;

public class BinarySearch {

	public static void main(String[] args) {

		int[] arr = { 1, 2, 3, 4, 5, 6, 7, 8, 9 };

		System.out.println(binarySearch(arr, -1, 0, arr.length - 1));

	}

	private static String binarySearch(int[] arr, int key, int left, int right) {
		int mid;

		while (left <= right) {
			mid = (left + right) / 2;

			if (arr[mid] == key)
				return "found at " + mid;

			if (key < arr[mid])
				right = mid - 1;
			else
				left = mid + 1;

		}

		return "Not present";
	}

}
