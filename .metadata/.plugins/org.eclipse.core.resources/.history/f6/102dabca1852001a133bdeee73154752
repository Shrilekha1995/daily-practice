	public static void main(String[] args) {
		int t=5;
		int number=1;
		int i=1;
		do{
			if(i%2 == 0){
				number+=i-1;
				for(int j=0;j<i;j++){
					System.out.print(number+" ");
					number--;
				}
				System.out.println();
				number+=i;
			}
			else{
				for(int j=0;j<i;j++){
					number+=1;
					System.out.print(number+" ");
					number++;
				}
				System.out.println();
			}
			i++;
		}while(t!=i);
	}
}
