package datastructure;

public class SelectionSort {

	public static void main(String[] args) {
		
		int [] arr={1,3,4,2,9,6};
		
		int min_index=arr[0];
		int temp;
		
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length-1;j++)
			{
				if(arr[min_index]>arr[j])
					min_index=j;
			}
			
			temp=arr[i];
			arr[i]=arr[min_index];
			arr[min_index]=temp;
			
		}

	}

}
