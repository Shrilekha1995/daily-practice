
public class AlternateRowPattern {

	public static void main(String[] args) {
		int k=1;
		
		for(int i=1;i<=4;i++)
		{
			
			if(i%2!=0)
			{
			for(int j=1;j<=i;j++)
			{
				
			System.out.print(k++);
			}
			}
			
			else{
				
				for(int t=i;t>=1;t--){
					System.out.println(t);
					System.out.print(k++);
				}
			}
			System.out.println();
		}
	}
}
